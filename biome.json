{
	"$schema": "https://biomejs.dev/schemas/1.2.2/schema.json",
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		},
		"ignore": [
			"dist",
			"docs"
		]
	},
	"formatter": {
		"formatWithErrors": true,
		"ignore": [
			"dist",
			"docs"
		]
	}
}